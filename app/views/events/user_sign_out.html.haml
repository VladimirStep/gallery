.page-header
  %h1.text-center
    = @user.email
    sign_ins
%table.table
  %thead
    %th Date/time
    %th Url
  %tbody
    - @actions.each do |action|
      %tr
        %td= action.created_at
        %td= action.url
%nav{'aria-label' => 'Page navigation', class: 'text-center'}
  = paginate @actions
