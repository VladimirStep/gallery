.page-header
  %h1.text-center
    = @user.email
    = t('.sign_outs')
%table.table
  %thead
    %th= t('.date_time')
    %th= t('.url')
  %tbody
    - @actions.each do |action|
      %tr
        %td= action.created_at
        %td= action.url
%nav{'aria-label' => 'Page navigation', class: 'text-center'}
  = paginate @actions
