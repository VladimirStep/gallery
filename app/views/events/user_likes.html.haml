.page-header
  %h1.text-center
    = @user.email
    = t('.likes')
%table.table
  %thead
    %th= t('.date_time')
    %th= t('.picture')
  %tbody
    - @likes.each do |like|
      %tr
        %td= like.created_at
        %td= like.picture_id
%nav{'aria-label' => 'Page navigation', class: 'text-center'}
  = paginate @likes
